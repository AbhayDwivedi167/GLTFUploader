{
  "database": {
    "rules": "database.rules.json"
  },
  "hosting": {
    "public": "dist\\GLTFUploader",
    "ignore": [
      "firebase.json",
      "**/.*",
      "**/node_modules/**"
    ]
  },
  "storage": {
    "rules": "storage.rules"
  },
  "rewrites": [
  {
    "source": "**",
    "destination": "/index.html"
  }  
  ]
}
